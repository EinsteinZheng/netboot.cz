DEFAULT vesamenu.c32
PROMPT 0
MENU BACKGROUND oskarotux-bg.png
MENU TITLE Welcome to boot.oskarcz.net

LABEL bootlocal
	MENU DEFAULT
	MENU LABEL Local Boot
	TEXT HELP
		This will exit from the network boot menu and attempt
		to boot from local media (hard disk, DVD, etc)
	ENDTEXT
	localboot 0x80

menu include http://dl.fedoraproject.org/pub/alt/bfo/pxelinux.cfg/default Fedora
menu include debian.conf Debian
